<link rel="stylesheet" href="/css/login.css" />

<div class="container">
  <div class="d-flex justify-content-center h-100">
    <div class="card">
      <div class="card-header"><h3>Report</h3></div>
      <div class="card-body">
        <h2>Name: <%= report.name %></h2>
        <h2>Status: <%= report.status %></h2>
